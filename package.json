    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/jest": "^26.4.24",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
