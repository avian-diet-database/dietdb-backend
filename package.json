    "startTS": "ts-node src/index.ts",
    "watch": "tsc -w",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "jest": {
    "testURL": "http://localhost:4000/graphql",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/_test_/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "verbose": true
