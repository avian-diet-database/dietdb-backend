    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "graphql-request": "^3.6.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
